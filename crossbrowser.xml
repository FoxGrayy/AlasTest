			<class name="test.tests.StudentTest" />
			<class name="test.tests.TeacherTest" />
			<class name="test.tests.CourseTest" />
		</classes>
	</test> <!-- Test -->
	<test thread-count="5" name="TestEdge">

		<parameter name="browser" value="Edge"></parameter>
		<classes>
			<class name="test.tests.base.FunctionalTest" />
			<class name="test.tests.StudentTest" />
