<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">
	<listeners>
		<listener class-name="test.tests.TestListener"/>
	</listeners>
	<test thread-count="5" name="TestChrome">
		<parameter name="browser" value="chrome"></parameter>
		<classes>
			<class name="test.tests.base.FunctionalTest" />
			<class name="test.tests.StudentTest" />
			<class name="test.tests.TeacherTest" />
			<class name="test.tests.CourseTest" />
		</classes>
	</test> <!-- Test -->
	<test thread-count="5" name="TestFfirefox">
